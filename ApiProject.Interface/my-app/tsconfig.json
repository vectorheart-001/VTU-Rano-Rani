{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"target": "es2021",
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": false ,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true
	}
	
}